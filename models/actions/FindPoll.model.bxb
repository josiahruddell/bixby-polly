action (FindPoll) {
  type(Search)
  description (Disambiguate a poll from a fuzzy query)
  collect {
    input (search) {
      type (SearchText)
      min (Optional) max (One)
    }
  }
  output (Name) {
    throws {
      error (user-has-not-voted) {
        on-catch {
          halt { dialog { template ("can't view this poll because you haven't voted") } }
        }
      }
    }
  }
}
